procedure TestLogOps;
begin
  BeginTest('TestLogOps');

  AssertBoolean('false and false', false, false and false);
  AssertBoolean('false and true', false, false and true);
  AssertBoolean('true and false', false, true and false);
  AssertBoolean('true and true', true, true and true);

  AssertBoolean('false or false', false, false or false);
  AssertBoolean('false or true', true, false or true);
  AssertBoolean('true or false', true, true or false);
  AssertBoolean('true or true', true, true or true);

  AssertBoolean('false xor false', false, false xor false);
  AssertBoolean('false xor true', true, false xor true);
  AssertBoolean('true xor false', true, true xor false);
  AssertBoolean('true xor true', false, true xor true);

  AssertBoolean('not false', true, not false);
  AssertBoolean('not true', false, not true);

  EndTest;
end;